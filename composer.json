{
  "name": "laralabs/geo-sorter",
  "description": "Postcode distance sorting package for Laravel Collections.",
  "license": "MIT",
  "authors": [
    {
      "name": "Matt Clinton",
      "email": "matt@laralabs.uk"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "ayeo/geo": "1.0.*",
    "illuminate/support": "^12.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^10.0",
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "Laralabs\\GeoSorter\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laralabs\\GeoSorter\\GeoSorterServiceProvider"
      ],
      "aliases": {
        "GetAddress": "Laralabs\\GeoSorter\\Facades\\GeoSorter"
      }
    }
  }
}
